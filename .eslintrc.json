{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [

  ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "process": true
  },
  "rules": {
    "linebreak-style": [
      "warn",
      "unix"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-empty": [
      "error",
      { "allowEmptyCatch": true }
    ]
  }
}